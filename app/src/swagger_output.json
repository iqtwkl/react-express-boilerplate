  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "loginInputSchema": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "example": "username"
          },
          "password": {
            "type": "string",
            "example": "somepassword"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "loginOutputSchema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "boolean",
            "example": true
          },
          "token": {
            "type": "string",
            "example": "token"
          },
          "refresh": {
            "type": "string",
            "example": "refresh-token"
          }
        }
      },
      "accountSchema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "uuid-hash"
          },
          "username": {
            "type": "string",
            "example": "unique-username"
          },
          "email": {
            "type": "string",
            "example": "mail@email.com"
          },
          "create_at": {
            "type": "string",
            "example": "yyyy-mm-dd"
          },
          "update_at": {
            "type": "string",
            "example": "yyyy-mm-dd"
          }
        }
      },
      "accountInSchema": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "example": "unique-username"
          },
          "password": {
            "type": "string",
            "example": "somepassword"
          },
          "email": {
            "type": "string",
            "example": "mail@email.com"
          }
        }
      },
      "listAccountSchema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "example": "uuid-hash"
            },
            "username": {
              "type": "string",
              "example": "unique-username"
            },
            "password": {
              "type": "string",
              "example": "somepassword"
            },
            "email": {
              "type": "string",
              "example": "mail@email.com"
            },
            "create_at": {
              "type": "string",
              "example": "yyyy-mm-dd"
            },
            "update_at": {
              "type": "string",
              "example": "yyyy-mm-dd"
            }
          }
        }
      },
      "profileInSchema": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "example": "John Doe"
          },
          "bio": {
            "type": "string",
            "example": "I am Human"
          },
          "avatarUrl": {
            "type": "string",
            "example": "/default-user.png"
          }
        }
      }
    }
